{
  "name": "claude-memory-backend",
  "version": "1.0.0",
  "description": "Multi-tenant AI conversation platform backend with per-user database isolation",
  "main": "dist/server.js",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:dev": "npm run build && npm start",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx src/scripts/migrate.ts",
    "db:migrate:prod": "node dist/scripts/migrate.js",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push:pg",
    "db:drop": "drizzle-kit drop",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "health": "curl http://localhost:3001/api/v1/health",
    "logs": "tail -f logs/combined.log"
  },
  "keywords": [
    "claude",
    "ai",
    "conversation",
    "multi-tenant",
    "backend",
    "api",
    "postgresql",
    "websocket"
  ],
  "author": "Claude Memory Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "cookie-parser": "^1.4.6",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "ws": "^8.14.2",
    "drizzle-orm": "^0.29.1",
    "postgres": "^3.4.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "nodemailer": "^6.9.7",
    "ioredis": "^5.3.2",
    "node-cron": "^3.0.3",
    "archiver": "^6.0.1",
    "csv-parse": "^5.5.2",
    "csv-stringify": "^6.4.4",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/cookie-parser": "^1.4.6",
    "@types/ws": "^8.5.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@types/node": "^20.10.4",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "@types/node-cron": "^3.0.11",
    "@types/archiver": "^6.0.2",
    "@types/mime-types": "^2.1.4",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "drizzle-kit": "^0.20.6",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "rimraf": "^5.0.5",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/claude-memory-backend.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/claude-memory-backend/issues"
  },
  "homepage": "https://github.com/your-org/claude-memory-backend#readme"
}